dhs:eau_de_cologne a gold:Noun,
        skos:Concept ;
    dct:bibliographicCitation "Sterenborg, W. en E. Schilders (2014), Woordenboek van de Tilburgse Taal (WTT)" ;
    skos:altLabel "oddeklonje"@nl,
